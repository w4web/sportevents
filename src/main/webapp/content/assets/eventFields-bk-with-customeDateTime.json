[
  {
    "key": "id",
    "type": "input",
    "className": "eventId",
    "defaultValue": 0,
    "templateOptions": {
      "label": "Id",
      "required": true
    },
    "expressionProperties": {
      "templateOptions.disabled": "model.id"
    }
  },
  {
    "fieldGroupClassName": "row",
    "fieldGroup": [
      {
        "className": "col-lg-6",
        "key": "eventType",
        "type": "select",
        "templateOptions": {
          "label": "Event type (Running/skeeler/shorttrack)",
          "options": [
            {
              "label": "Running",
              "value": "Running"
            },
            {
              "label": "Skeeler",
              "value": "skeeler"
            },
            {
              "label": "Shorttrack",
              "value": "shorttrack"
            }
          ],
          "required": true
        }
      },
      {
        "className": "col-lg-6 icon-right",
        "key": "dateTime",
        "type": "dateTime",
        "templateOptions": {
          "label": "Date & Time",
          "placeholder": "yyyy-mm-dd",
          "required": true
        }
      }
    ]
  },
  {
    "fieldGroupClassName": "row",
    "fieldGroup": [
      {
        "className": "col-lg-6 icon-right",
        "type": "input",
        "key": "peoples",
        "templateOptions": {
          "type": "text",
          "label": "Number of peoples",
          "placeholder": "Number of peoples",
          "addonRight": {
            "class": "far fa-user"
          }
        },
        "validators": {
          "validation": ["onlyNumber"]
        }
      },
      {
        "className": "col-lg-6 icon-right",
        "type": "input",
        "key": "location",
        "templateOptions": {
          "label": "Location",
          "placeholder": "Location",
          "required": true,
          "addonRight": {
            "class": "icon-marker"
          }
        }
      }
    ]
  },
  {
    "fieldGroupClassName": "row",
    "fieldGroup": [
      {
        "className": "col-lg-6",
        "type": "input",
        "key": "trainerName",
        "templateOptions": {
          "label": "Trainer name",
          "placeholder": "Trainer name",
          "required": true
        }
      },
      {
        "className": "col-lg-6",
        "key": "equipment",
        "type": "select",
        "templateOptions": {
          "label": "Equipment",
          "options": [
            {
              "label": "Equipment1",
              "value": "equipment1"
            },
            {
              "label": "Equipment2",
              "value": "equipment2"
            },
            {
              "label": "Equipment3",
              "value": "equipment3"
            }
          ],
          "required": true
        }
      }
    ]
  },
  {
    "fieldGroupClassName": "row",
    "fieldGroup": [
      {
        "className": "col-lg-12",
        "type": "textarea",
        "key": "description",
        "templateOptions": {
          "label": "Description",
          "placeholder": "Description",
          "required": true
        }
      }
    ]
  }
]
