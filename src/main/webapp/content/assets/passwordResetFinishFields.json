[
  {
    "validators": {
      "validation": [
        {
          "name": "fieldMatch",
          "options": {
            "errorPath": "passwordConfirm"
          }
        }
      ]
    },
    "fieldGroup": [
      {
        "key": "password",
        "type": "input",
        "templateOptions": {
          "type": "password",
          "label": "New Password",
          "placeholder": "Must be at least 4 characters",
          "required": true,
          "minLength": 4
        }
      },
      {
        "key": "passwordConfirm",
        "type": "input",
        "templateOptions": {
          "type": "password",
          "label": "Confirm Password",
          "placeholder": "Please re-enter your password",
          "required": true
        }
      }
    ]
  }
]
